<div class="row-fluid">
  <div class="span12">
   <%= page_title(resource.title) %> 
  </div>
</div>
<div class="row-fluid">

  <div class="span7">
    <% if resource.body.present? %>
      <div class="page_content">
        <%= resource_body_tag do %>
          <%= render_html_text(resource.body) %>
        <% end %> 
      </div> 
    <% end %>
  </div> 
  <div class="span5">
      <%= render :partial => "shared/appointment_form" %>
  </div>
</div>